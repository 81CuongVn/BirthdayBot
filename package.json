{
    "name": "birthday-bot",
    "version": "4.3.0",
    "author": "Scott Bucher",
    "description": "Celebrate birthdays with automatic birthday roles and announcements! Highly customizable and easy to use!",
    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": ">=16.6.0"
    },
    "type": "module",
    "exports": [
        "./dist/start-bot.js",
        "./dist/start-manager.js"
    ],
    "scripts": {
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "clean": "git clean -xdf --exclude=\"/config/**/*\"",
        "clean:dry": "git clean -xdf --exclude=\"/config/**/*\" --dry-run",
        "build": "tsc --project tsconfig.json",
        "commands:register": "npm run build && node --enable-source-maps dist/start-bot.js --register",
        "commands:clear": "npm run build && node --enable-source-maps dist/start-bot.js --clear",
        "start": "npm run start:bot",
        "start:bot": "npm run build && node --enable-source-maps dist/start-bot.js",
        "start:manager": "npm run build && node --enable-source-maps dist/start-manager.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "@discordjs/rest": "0.4.1",
        "chrono-node": "^2.3.7",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "color-name-list": "^8.38.0",
        "discord.js-collector-utils": "^1.4.2",
        "discord-api-types": "0.33.5",
        "discord.js": "13.8.0",
        "discord.js-rate-limiter": "1.3.2",
        "express": "4.18.1",
        "express-promise-router": "4.1.1",
        "filesize": "8.0.7",
        "linguini": "1.3.1",
        "luxon": "2.4.0",
        "moment": "^2.29.2",
        "moment-timezone": "^0.5.34",
        "mysql": "^2.18.1",
        "node-fetch": "3.2.4",
        "node-schedule": "2.1.0",
        "pino": "7.11.0",
        "pino-pretty": "7.6.1",
        "pm2": "^5.2.0",
        "reflect-metadata": "^0.1.13",
        "remove-markdown": "0.5.0"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/luxon": "2.3.2",
        "@types/node": "^17.0.31",
        "@types/node-fetch": "3.0.2",
        "@types/node-schedule": "2.1.0",
        "@types/remove-markdown": "0.3.1",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.15.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-unicorn": "^42.0.0",
        "prettier": "^2.6.2",
        "typescript": "^4.6.4"
    }
}
